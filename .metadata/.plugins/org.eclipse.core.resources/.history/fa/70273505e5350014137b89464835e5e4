
Ti.UI.setBackgroundColor("white");

var mainWin = Ti.UI.createWindow({
	title: "Top 10 Comics",
	backgroundColor: "#6897bb",
});

var view1 = Ti.UI.createView({
	backgroundColor: "white",
	borderRadius: 2,
	borderWidth: 1,
	top: 20,
	left: 20,
	right: 20,
	bottom: "15%"
});

var label1 = Ti.UI.createLabel({
	text: "label 1",
	color: "black",
	font: {fontSize: 20},
	top: "50%",
	left: "50%",
	width: Ti.UI.FILL
});




mainWin.add(view1);
view1.add(label1);
mainWin.open();



















